@model List<TeachersModel>
 @*=====================*@
<div class="row row-cards row-deck">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Invoices</h3>
            </div>
            <div class="table-responsive">
                <table class="table card-table table-vcenter text-nowrap">
                    <thead>
                        <tr>
                            <th class="w-1">First Name</th>
                            <th>Middle Name</th>
                            <th>Last Name</th>

                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (TeachersModel tm in Model)
                        {
                            <tr>
                                <td>@tm.fname</td>
                                <td>@tm.mname</td>
                                <td>@tm.lname</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@*=====================*@
